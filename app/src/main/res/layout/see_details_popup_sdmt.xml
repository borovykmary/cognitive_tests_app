<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/small_see_details_popup" />

    <Button
        android:id="@+id/submitButton"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:background="@drawable/button_with_stroke"
        android:fontFamily="@font/poppins_semibold"
        android:paddingStart="28dp"
        android:paddingEnd="28dp"
        android:text="Close"
        android:textAllCaps="false"
        android:textColor="#778293"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15"
        app:layout_constraintEnd_toEndOf="@+id/imageView15"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/imageView15"
        app:layout_constraintTop_toTopOf="@+id/imageView15"
        app:layout_constraintVertical_bias="0.882" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="272dp"
        android:layout_height="61dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Symbol Digit Modalities Test Results"
        android:textAlignment="center"
        android:textColor="@color/green_haki"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15"
        app:layout_constraintEnd_toEndOf="@+id/imageView15"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="@+id/imageView15"
        app:layout_constraintTop_toTopOf="@+id/imageView15"
        app:layout_constraintVertical_bias="0.075" />

    <TextView
        android:id="@+id/dateSDMC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginBottom="15dp"
        android:fontFamily="@font/poppins_regular"
        android:text="Date: 12.04.2024"
        android:textColor="@color/green_haki"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/score"
        app:layout_constraintStart_toStartOf="@+id/imageView15" />

    <TextView
        android:id="@+id/score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="86dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Score"
        android:textColor="@color/green_haki"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/totalSDMC"
        app:layout_constraintEnd_toEndOf="@+id/dateSDMC"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/dateSDMC" />

    <TextView
        android:id="@+id/totalSDMC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="100dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Total: 56/6"
        android:textColor="@color/green_haki"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15"
        app:layout_constraintStart_toStartOf="@+id/score" />

    <TextView
        android:id="@+id/totalSDMCAll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Total Answers:"
        android:textColor="@color/green_haki"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/totalSDMCCorrect"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/totalSDMCCorrect"
        app:layout_constraintTop_toBottomOf="@+id/totalSDMCCorrect" />

    <TextView
        android:id="@+id/totalSDMCCorrect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Correct Answers:"
        android:textColor="@color/green_haki"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/score"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/score"
        app:layout_constraintTop_toBottomOf="@+id/score" />

</androidx.constraintlayout.widget.ConstraintLayout>